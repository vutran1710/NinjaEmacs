# -*- mode: snippet -*-
# name: testSubclass
# key: testSubclass
# --
define([
	'./$1-tests'
], function($1Tests) {
	/**
	 * Tests for \``(replace-regexp-in-string "\\(.*\\)-tests" "\\1" (file-name-sans-extension (file-name-nondirectory (buffer-file-name))))`\`.
  	 */
	return $1Tests.subclass((that, my) => {
		my.name = function() {
			return '$2';
		};

		$0
	});
});
